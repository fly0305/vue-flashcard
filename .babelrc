{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "debug": true,
                "useBuiltIns": "entry",
                "corejs": {
                    "version": 3,
                    "proposals": true
                },
                "targets": "last 2 versions, > 0.2%, not dead"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import"
    ]
}
